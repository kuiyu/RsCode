<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <OutputType>Library</OutputType>

	  <Version>2.1.00</Version>
	  <Authors>Tony</Authors>
	  <Company>河南软商网络科技有限公司</Company>
	  <PackageIcon>rscode64_64.png</PackageIcon>
	  <Description>https://rscode.cn</Description>
	  <Copyright>河南软商网络科技有限公司</Copyright>
	  <PackageProjectUrl>https://rscode.cn</PackageProjectUrl>
	  <PackageReleaseNotes>软商网络asp.net core项目开发工具包</PackageReleaseNotes>
	  <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	  <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
	  <AssemblyVersion>2.1.0.0</AssemblyVersion>
	  <FileVersion>2.1.0.0</FileVersion>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <RepositoryUrl>https://github.com/kuiyu/RsCode.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.33" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RsCode\RsCode.csproj" />
  </ItemGroup>

</Project>
